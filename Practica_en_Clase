CREATE TABLE `TALLER_1`.`Category`(
`id` int NOT NULL AUTO_INCREMENT,
`Name` varchar (50) Not Null,
PRIMARY KEY(`id`)
);

CREATE TABLE `TALLER_1`.`Furniture`(
`id` int NOT NULL AUTO_INCREMENT,
`Manufacture_Date` date NOT NULL,
PRIMARY KEY(`id`)
);

CREATE TABLE `TALLER_1`.`Food`(
`id` int NOT NULL AUTO_INCREMENT,
`Expiration_Date` date NOT NULL,
`Calories` varchar (50) Not Null,
PRIMARY KEY(`id`)
);

CREATE TABLE `TALLER_1`.`Order_Line`(
`id` int NOT NULL AUTO_INCREMENT,
`Quantity` varchar (50) Not Null,
PRIMARY KEY(`id`)
);

CREATE TABLE `TALLER_1`.`Order`(
`id` int NOT NULL AUTO_INCREMENT,
PRIMARY KEY(`id`)
);

CREATE TABLE `TALLER_1`.`Product`(
`id` int NOT NULL AUTO_INCREMENT,
`Name` varchar (50) Null,
`Price` float NOT NULL,
PRIMARY KEY(`id`)
);
